<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bluemoonl.ch26snsupload">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CH14USEDGOODSTRADEAPP"
        android:name=".SnsUploadApplication">
        <activity
            android:name="com.bluemoonl.ch26snsupload.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.AddArticleActivity" />
        <activity android:name=".chatdetail.ChatRoomActivity" />
        <activity android:name=".photo.CameraActivity" />
        <activity android:name=".photo.ImagePreviewListActivity" />
        <activity android:name=".gallery.GalleryActivity"/>
    </application>

</manifest>